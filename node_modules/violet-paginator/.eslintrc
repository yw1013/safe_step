{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": ["react"],
  "rules": {
    "new-cap": [2, { "capIsNewExceptions": ["List", "Map", "Set"] }],
    "semi": [2, "never"],
    "comma-dangle": [2, "never"],
    "no-constant-condition": ["off"],
    "space-infix-ops": ["off"],
    "no-unused-vars": ["error", { "varsIgnorePattern": "^_" }],
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  }
}
